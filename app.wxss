/**app.wxss**/
@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_yviu935dzy.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_yviu935dzy.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_yviu935dzy.woff2') format('woff2'),
  url('//at.alicdn.com/t/webfont_yviu935dzy.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_yviu935dzy.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_yviu935dzy.svg#思源黑体-极细') format('svg'); /* iOS 4.1- */
}

page {
   font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  background: #F2F2F4;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
